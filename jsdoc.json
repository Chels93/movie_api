{
    "source": {
      "include": [
        "index.js",
        "router-movies.js",
        "router-users.js",
        "auth.js",
        "models.js",
        "passport.js"
      ],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs/)"
    },
    "opts": {
      "destination": "./public/docs",
      "recurse": true,
      "readme": "README.md"
    },
    "plugins": [
      "plugins/markdown"
    ],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    }
  }
  